        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <RootNamespace>Jaahas.WriteAheadLog.Grpc</RootNamespace>
    </PropertyGroup>

    <ItemGroup>
        <Protobuf Include="Protos\wal.proto" GrpcServices="Server" />
